<div id="table_WordLogs" class="table_div">
    <table class="store_findResultBox" border="1" cellpadding="0" id="GeneralOperation_WorkLog"></table>
    <div class="store_changePage">
        <button id="GeneralOperation_butPre" class="store_changePage_button" onclick="GeneralOperationChangePre()">上一页</button>
        <a class="store_changePage_info">第 <span id="GeneralOperation_nowPage"></span> 页</a>
        <button id="GeneralOperation_butNext" class="store_changePage_button" onclick="GeneralOperationChangeNext()">下一页</button>
        <a class="store_changePage_info">共 <span id="GeneralOperation_totalPage">@Model.pageTotal</span> 页，到</a>
        <input id="GeneralOperation_page" type="number" />
        <a class="store_changePage_info">页</a>
        <button onclick="GeneralOperationGoPageNum()">确定</button>
    </div>
</div>
